.wx-popup {
  position: fixed;
  z-index: 100;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.popup-container {
  position: fixed;
  z-index: 999;
  top: 100rpx;
  left: 100rpx;
  width: 183rpx;
  border: 2rpx solid #ccc;
  border-radius: 8rpx;
  background-color: #fff
}

.item-container {
  width: 100%;
  height: 70rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.item-container text {
  font-size: 23rpx;
  color: #333;
}

.row-line {
  color: #333;
  width: 100%;
  height: 1rpx;
  margin: 0rpx 19rpx;
}
